{
  "name": "Hive",
  "description": "ecommerce channel manager",
  "author": "Salsita Software (https://www.salsitasoft.com/)",
  "version": "3.2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/salsita/hive"
  },
  "homepage": "https://getvela.com",
  "license": "UNLICENSED",
  "private": true,
  "engines": {
    "node": ">=8.9.0",
    "yarn": ">=1.1.0"
  },
  "scripts": {
    "preinstall": "node -e 'if(process.env.npm_execpath.indexOf(\"yarn\") === -1) { console.error(\"npm not supported anymore, use yarn\"); process.exit(1);}'",
    "prepush": "yarn run tests",
    "migrate_app_db": "node_modules/node-pg-migrate/bin/pg-migrate --check-order -m ./migrations-app -v up",
    "migrate_auth_db": "node_modules/node-pg-migrate/bin/pg-migrate --check-order -m ./migrations-auth -v up",
    "build_all": "yarn run gen_ver_file && yarn run build_web && yarn run build_manager && yarn run build_auth && yarn run build_worker && yarn run build_handler",
    "build_web": "yarn run build_web_frontend && yarn run build_web_backend",
    "build_worker": "yarn run build_worker_backend",
    "build_handler": "yarn run build_handler_backend",
    "build_manager": "yarn run build_manager_backend",
    "build_auth": "yarn run build_auth_frontend && yarn run build_auth_backend",
    "build_web_frontend": "bin/webpack_wrap.sh webpack/web.frontend.config.js",
    "build_web_backend": "bin/webpack_wrap.sh webpack/web.backend.config.js",
    "build_auth_frontend": "bin/webpack_wrap.sh webpack/auth.frontend.config.js",
    "build_auth_backend": "bin/webpack_wrap.sh webpack/auth.backend.config.js",
    "build_manager_backend": "bin/webpack_wrap.sh webpack/manager.backend.config.js",
    "build_worker_backend": "bin/webpack_wrap.sh webpack/worker.backend.config.js",
    "build_handler_backend": "bin/webpack_wrap.sh webpack/handler.backend.config.js",
    "lint": "yarn run lint_shared && yarn run lint_web && yarn run lint_auth && yarn run lint_manager && yarn run lint_worker && yarn run lint_test && yarn run lint_handler",
    "lint_shared": "./node_modules/.bin/eslint -c .eslintrc ./shared",
    "lint_web": "./node_modules/.bin/eslint -c .eslintrc ./web/src",
    "lint_auth": "./node_modules/.bin/eslint -c .eslintrc ./auth/src",
    "lint_manager": "./node_modules/.bin/eslint -c .eslintrc ./manager/src",
    "lint_worker": "./node_modules/.bin/eslint -c .eslintrc ./worker/src",
    "lint_handler": "./node_modules/.bin/eslint -c .eslintrc ./handler/src",
    "lint_test": "./node_modules/.bin/eslint -c .eslintrc ./test",
    "dev_web": "MODULE=web ./node_modules/.bin/babel-node devRunner.js",
    "dev_auth": "MODULE=auth ./node_modules/.bin/babel-node devRunner.js",
    "dev_manager": "MODULE=manager CLIENT=false ./node_modules/.bin/babel-node devRunner.js",
    "dev_worker": "MODULE=worker CLIENT=false ./node_modules/.bin/babel-node devRunner.js",
    "dev_handler": "MODULE=handler CLIENT=false ./node_modules/.bin/babel-node devRunner.js",
    "test_backend": "yarn run test_shared && yarn run test_auth_backend && yarn run test_web_backend && yarn run test_manager_backend && yarn run test_worker_backend && yarn run test_test && yarn run test_handler_backend",
    "test_shared": "bin/test_wrapper.sh . shared webpack/shared.config.js ./shared/test-results.xml",
    "test_auth_backend": "bin/test_wrapper.sh . auth/src/server webpack/auth.backend.config.js",
    "test_web_backend": "bin/test_wrapper.sh . web/src/server webpack/web.backend.config.js",
    "test_test": "bin/test_wrapper.sh . test webpack/shared.config.js ./test/test-results.xml",
    "test_manager_backend": "bin/test_wrapper.sh . manager/src webpack/manager.backend.config.js ./manager/test-results.xml",
    "test_worker_backend": "bin/test_wrapper.sh . worker/src webpack/worker.backend.config.js ./worker/test-results.xml",
    "test_handler_backend": "bin/test_wrapper.sh . handler/src webpack/handler.backend.config.js ./handler/test-results.xml",
    "test_frontend": "yarn run test_auth_frontend && yarn run test_web_frontend",
    "test_auth_frontend": "cd auth && karma start",
    "test_web_frontend": "cd web && karma start",
    "tests": "yarn run lint && yarn run test_backend && yarn run test_frontend",
    "build_artifacts": "bin/create_artifacts.sh",
    "gen_ver_file": "bin/gen_ver_json.sh > buildVersion.json"
  },
